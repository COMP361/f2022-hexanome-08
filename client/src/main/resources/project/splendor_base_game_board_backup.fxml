<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="playerBoardAnchorPane" maxHeight="900.0" maxWidth="1600.0"
