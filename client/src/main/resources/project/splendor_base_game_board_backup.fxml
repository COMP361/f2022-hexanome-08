        </VBox>
        <VBox fx:id="orientCardBoard" layoutX="730.0" layoutY="230.0" prefHeight="390.0" prefWidth="250.0">
            <children>
                <HBox fx:id="level3OriebtCard" prefHeight="130.0" prefWidth="250.0">
                    <children>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="130.0" stroke="BLACK"
