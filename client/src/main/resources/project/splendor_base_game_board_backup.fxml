            prefWidth="1600.0" style="-fx-background-color: #ffebcd;"
            xmlns="http://javafx.com/javafx/19">
    <children>
        <VBox fx:id="nobleList" layoutX="1000.0" layoutY="150.0" prefHeight="500.0" prefWidth="100.0">
            <children>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="500.0" stroke="BLACK"
                           strokeType="INSIDE" width="100.0"/>
            </children>
        </VBox>
        <HBox fx:id="token" layoutX="190.0" layoutY="133.0" prefHeight="130.0" prefWidth="750.0"/>
        <VBox fx:id="baseCardBoard" layoutX="250.0" layoutY="230.0" prefHeight="390.0" prefWidth="455.0">
            <children>
                <HBox fx:id="level3BaseCard" prefHeight="130.0" prefWidth="415.0"/>
                <HBox fx:id="level2BaseCard" prefHeight="130.0" prefWidth="415.0"/>
                <HBox fx:id="level1BaseCard" prefHeight="130.0" prefWidth="415.0"/>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="130.0" stroke="BLACK"
                           strokeType="INSIDE" width="455.0"/>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="130.0" stroke="BLACK"
                           strokeType="INSIDE" width="455.0"/>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="130.0" stroke="BLACK"
                           strokeType="INSIDE" width="455.0"/>
