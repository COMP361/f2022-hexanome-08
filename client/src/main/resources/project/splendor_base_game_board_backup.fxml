            <children>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="100.0" stroke="BLACK"
                           strokeType="INSIDE" width="750.0"/>
            </children>
        </HBox>
        <VBox layoutX="1120.0" layoutY="125.0" prefHeight="200.0" prefWidth="100.0">
            <children>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="500.0" stroke="BLACK"
                           strokeType="INSIDE" width="200.0"/>
            </children>
        </VBox>
