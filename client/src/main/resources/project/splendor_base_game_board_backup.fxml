        <HBox fx:id="topPlayer" alignment="TOP_RIGHT" layoutX="468.0" maxHeight="125.0" maxWidth="665.0"
              prefHeight="125.0" prefWidth="665.0" BorderPane.alignment="TOP_RIGHT">
            <children>
                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="125.0" stroke="BLACK"
                           strokeType="INSIDE" width="665.0"/>
            </children>
        </HBox>
        <VBox fx:id="leftPlayer" alignment="BOTTOM_LEFT" layoutY="118.0" maxHeight="665.0" maxWidth="125.0"
