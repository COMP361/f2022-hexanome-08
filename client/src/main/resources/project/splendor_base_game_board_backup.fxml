                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="665.0" stroke="BLACK"
                           strokeType="INSIDE" width="125.0"/>
            </children>
        </VBox>
        <VBox fx:id="rightPlayer" alignment="BOTTOM_LEFT" layoutX="1350.0" layoutY="118.0" maxHeight="800.0"
