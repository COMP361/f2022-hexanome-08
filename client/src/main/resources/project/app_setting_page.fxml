            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="77.0" prefWidth="426.0" text="play local game" textAlignment="CENTER">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
            <HBox prefHeight="27.0" prefWidth="384.0">
               <children>
                  <Region prefHeight="27.0" prefWidth="146.0" />
