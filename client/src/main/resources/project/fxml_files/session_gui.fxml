<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
         minHeight="-Infinity"
         minWidth="-Infinity" prefHeight="140.0" prefWidth="840.0"
         style="-fx-border-width: 6; -fx-border-color: lightblue;"
         type="HBox" xmlns="http://javafx.com/javafx/19">
    <children>
        <AnchorPane prefHeight="128.0" prefWidth="700.0">
            <children>
                <Label fx:id="gameNameLabel" layoutX="8.0" prefHeight="41.0" prefWidth="329.0" text="Game Display"
                       textFill="#00aeff">
                    <font>
                        <Font name="MesloLGS NF Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label fx:id="creatorNameLabel" layoutX="8.0" layoutY="40.0" prefHeight="26.0" prefWidth="558.0"
                       text="creator: 123" textFill="#75c8fa">
                    <font>
                        <Font name="MesloLGS NF Bold Italic" size="14.0"/>
                    </font>
                </Label>
                <Label fx:id="playerInfosLabel" layoutX="8.0" layoutY="60.0" prefHeight="41.0" prefWidth="586.0"
                       text="players: []" textFill="#75c8fa">
                    <font>
                        <Font name="MesloLGS NF Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label fx:id="saveGameIdLabel" layoutX="8.0" layoutY="100.0" prefHeight="26.0" prefWidth="513.0"
                       text="save game id:" textFill="#00a1ff">
                    <font>
                        <Font name="MesloLGS NF Bold Italic" size="14.0"/>
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Region prefHeight="128.0" prefWidth="178.0"/>
        <VBox fx:id="buttonsVbox" prefHeight="140.0" prefWidth="175.0" spacing="10.0">
            <children>
                <Button fx:id="topButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="177.0" text="Button"
                        visible="false"/>
                <Button fx:id="btmButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="215.0" text="Button"
                        visible="false"/>
            </children>
        </VBox>
    </children>
</fx:root>
