<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="backgroundPane" prefHeight="600.0" prefWidth="1100.0"
            xmlns="http://javafx.com/javafx/19">
    <children>
        <HBox layoutX="210.0" layoutY="40.0" prefHeight="507.0" prefWidth="670.0">
            <children>
                <VBox prefHeight="405.0" prefWidth="311.0" style="-fx-border-color: blue; -fx-border-width: 8;">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="77.0" prefWidth="426.0"
                               text="Use Default Login Info" textAlignment="CENTER">
                            <font>
                                <Font name="Arial Black" size="18.0"/>
                            </font>
                        </Label>
                        <HBox prefHeight="27.0" prefWidth="384.0">
                            <children>
                                <Region prefHeight="27.0" prefWidth="99.0"/>
                                <ToggleButton fx:id="defaultLogInToggleButton" mnemonicParsing="false" prefHeight="26.0"
                                              prefWidth="99.0" text="OFF"/>
                            </children>
                        </HBox>
                        <Region prefHeight="35.0" prefWidth="223.0"/>
                        <HBox prefHeight="28.0" prefWidth="384.0">
                            <children>
                                <Label prefHeight="17.0" prefWidth="87.0" text="user name">
                                    <font>
                                        <Font name="Arial Black" size="15.0"/>
                                    </font>
                                </Label>
                                <TextField fx:id="defaultUserNameTextField" prefHeight="26.0" prefWidth="139.0"/>
                                <Button fx:id="defaultUserNameUpdateButton" mnemonicParsing="false" prefHeight="26.0"
                                        prefWidth="69.0" text="Update"/>
                            </children>
                        </HBox>
                        <Region prefHeight="76.0" prefWidth="384.0"/>
                        <HBox prefHeight="28.0" prefWidth="384.0">
                            <children>
                                <Label prefHeight="17.0" prefWidth="87.0" text="password">
                                    <font>
                                        <Font name="Arial Black" size="15.0"/>
                                    </font>
                                </Label>
                                <PasswordField fx:id="defaultPasswordField" prefHeight="26.0" prefWidth="145.0"/>
                                <Button fx:id="defaultPasswordUpdateButton" mnemonicParsing="false" prefHeight="26.0"
                                        prefWidth="69.0" text="Update"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <Region prefHeight="507.0" prefWidth="77.0"/>
                <VBox prefHeight="507.0" prefWidth="282.0">
                    <children>
                        <VBox prefHeight="84.0" prefWidth="282.0"
                              style="-fx-border-color: orange; -fx-border-width: 8;">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="34.0" prefWidth="266.0"
                                       text="Music Volume" textAlignment="CENTER">
                                    <font>
                                        <Font name="Arial Black" size="18.0"/>
                                    </font>
                                </Label>
                                <HBox prefHeight="24.0" prefWidth="266.0">
                                    <children>
                                        <TextField fx:id="musicVolumeTextField" alignment="CENTER" prefHeight="26.0"
                                                   prefWidth="69.0">
                                            <font>
                                                <Font size="15.0"/>
                                            </font>
                                        </TextField>
                                        <Slider fx:id="musicVolumeSlider" prefHeight="26.0" prefWidth="197.0"/>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <Region prefHeight="89.0" prefWidth="282.0"/>
                        <VBox prefHeight="251.0" prefWidth="295.0" style="-fx-border-color: blue; -fx-border-width: 8;">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="77.0" prefWidth="426.0"
                                       text="Play Local Game" textAlignment="CENTER">
                                    <font>
                                        <Font name="Arial Black" size="18.0"/>
                                    </font>
                                </Label>
                                <HBox prefHeight="27.0" prefWidth="384.0">
                                    <children>
                                        <Region prefHeight="27.0" prefWidth="90.0"/>
                                        <ToggleButton fx:id="localIpToggleButton" mnemonicParsing="false"
                                                      prefHeight="26.0" prefWidth="99.0" text="OFF"/>
                                    </children>
                                </HBox>
                                <Region prefHeight="66.0" prefWidth="384.0"/>
                                <HBox prefHeight="28.0" prefWidth="384.0">
                                    <children>
                                        <Label prefHeight="17.0" prefWidth="87.0" text="host ip">
                                            <font>
                                                <Font name="Arial Black" size="15.0"/>
                                            </font>
                                        </Label>
                                        <TextField fx:id="defaultHostIpTextField" prefHeight="26.0" prefWidth="156.0"/>
                                        <Button fx:id="hostIpUpdateButton" mnemonicParsing="false" prefHeight="26.0"
                                                prefWidth="98.0" text="Update"/>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <Region prefHeight="90.0" prefWidth="282.0"/>
                        <Button fx:id="backToLogInButton" mnemonicParsing="false" prefHeight="41.0" prefWidth="283.0"
                                style="-fx-background-color: blue;" text="Back To Log In Page" textFill="WHITE">
                            <font>
                                <Font name="Arial Black" size="19.0"/>
                            </font>
                        </Button>
                    </children>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
